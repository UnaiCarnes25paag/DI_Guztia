<Window x:Class="Ariketa2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ariketa2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="FraseTextBox" HorizontalAlignment="Left" Height="25" Margin="113,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="550"/>
        <Grid HorizontalAlignment="Left" Height="155" Margin="113,120,0,0" VerticalAlignment="Top" Width="550">
            <Button Click="FraseButton_Click" x:Name="Frase1Button" Content="Frase1" HorizontalAlignment="Left" Height="40" Margin="10,10,0,0" VerticalAlignment="Top" Width="110"/>
            <Button Click="FraseButton_Click" x:Name="Frase2Button" Content="Frase2" HorizontalAlignment="Center" Height="40" Margin="0,10,0,0" VerticalAlignment="Top" Width="110"/>
            <Button Click="FraseButton_Click" x:Name="Frase3Button" Content="Frase3" HorizontalAlignment="Left" Height="40" Margin="430,10,0,0" VerticalAlignment="Top" Width="110"/>
            <Button Click="FraseButton_Click" x:Name="Frase4Button" Content="Frase4" HorizontalAlignment="Left" Height="40" Margin="10,91,0,0" VerticalAlignment="Top" Width="110"/>
            <Button Click="FraseButton_Click" x:Name="Frase5Button" Content="Frase5" HorizontalAlignment="Center" Height="40" Margin="0,91,0,0" VerticalAlignment="Top" Width="110"/>
            <Button Click="UnirButton_Click" x:Name="UnirButton" Content="Unir" HorizontalAlignment="Left" Height="40" Margin="430,86,0,0" VerticalAlignment="Top" Width="110"/>
        </Grid>
        <Button Click="LimpiarButton_Click" x:Name="LimpiarButton" Content="Limpiar" HorizontalAlignment="Left" Height="35" Margin="125,296,0,0" VerticalAlignment="Top" Width="538"/>
        <Button Click="SalirButton_Click" x:Name="SalirButton" Content="Salir" HorizontalAlignment="Left" Height="35" Margin="125,360,0,0" VerticalAlignment="Top" Width="538"/>

    </Grid>
</Window>
